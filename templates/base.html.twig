<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Chaco tv{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        
        {% block stylesheets %}
            <!--LOGO-->
            <link rel="icon" href="{{asset('build/img/isologo.svg')}}" type="img/svg"/>   
            <!-- GOOGLE FONTS -->
            <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;800&display=swap" rel="stylesheet">
            <!-- BOOTSTRAP CSS -->
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU" crossorigin="anonymous"> 
            <!-- LINK CSS -->
            {# <link rel="stylesheet" href="{{asset('build/css/app.css')}}"> #}
            <!-- bootstrap -->
             <link rel="stylesheet" href="{{asset('build/css/maqueta/estilos.css')}}"> 
             {# {{ encore_entry_link_tags('app') }} #}
        {% endblock %}

        
        {# {% block javascripts %}
            {{{ encore_entry_script_tags('app') }} 
        {% endblock %}  #}
    </head>
    <body>
        {% block headers %}
            <header>
                <div class="headerRedes">
                    <div class="enlaceRedes justify-content-end align-items-end text-center">
                        <a href="https://twitter.com/ChacoTv" target="_blank"><img class="twitter" src="{{asset('build/img/twN.svg')}}"alt="logo de twitter"></a>
                        <a href="https://www.facebook.com/chacotelevision" target="_blank"><img  class="facebook" src="{{asset('build/img/fbN.svg')}}" alt="logo de discord"></a>
                        <a href="https://www.instagram.com/chacotv/" target="_blank"><img  class="instagram" src="{{asset('build/img/igN.svg')}}" alt="logo de telegram"></a>
                        <a href="https://www.youtube.com/user/tvpublicachaco" target="_blank"><img  class="youtube" src="{{asset('build/img/ybN.svg')}}" alt="logo de telegram"></a>
                    </div>
                    <div class="logos justify-content-center align-items-end text-center">
                        <a href="http://ecom.chaco.gov.ar/es/" target="_blank"><img class="ecom" src="{{asset('build/img/ecom.svg')}}"alt="logo ecom"></a>
                        <a href="https://agenciafoco.com.ar/" target="_blank"><img class="foco" src="{{asset('build/img/focoN.svg')}}"alt="logo foco"></a>
                        <a href="https://radioprovinciadelchaco.com.ar/" target="_blank"><img class="radio" src="{{asset('build/img/radioN.svg')}}"alt="logo radio"></a>
                    </div>
                </div>
            </header>
        {% endblock %}
        {% block nav %}
            <nav class="navbar navbar-expand-xxl navbar-light bg miMenu">
                <div class="container-fluid">
                    <a class="navbar-brand px-3" href="{{path('main_page')}}">
                        <img src="{{asset('build/img/logoChtvNegro.svg')}}" alt="logo ecomPool" width="170" height="55">
                    </a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span><img id="menuHam" src="{{asset('build/img/menu.svg')}}" alt="" width="20" height="20" style="border: none; box-shadow: none;" ></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-center" id="navbarSupportedContent">
                        <div class="align-items-center justify-content-center text-center" >
                            <a href="./pages/vivo.html"><button class="buttonVivo">VIVO<img class="iconoNavBar" src="{{asset('build/img/vivo.svg')}}"alt="iconoVivo"></button></a>
                        </div>
                        <ul class="navbar-nav align-items-center itemsNavegacion">
                            <li class="nav-item">
                                <a class="nav-link" aria-current="page" href="{{path('main_page')}}">Inicio</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" aria-current="page" href="./pages/programas.html">Programas</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" aria-current="page" href="./pages/horario.html">Horarios</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" aria-current="page" href="./pages/contacto.html">Contacto</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav> 
        {% endblock %}
        {% block main %}{% endblock %}
        {% block footer_links %}
            <footer class="py-4">
                <div class="container">
                 <div class="row">
                    <div class="col-sm-12  col-md-12 col-lg-4 text-center" >
                        <figure class="logo">
                            <div class="row">
                                <div class="col-6  justify-content-center text-center">
                                    <a href="https://twitter.com/ChacoTv"><img class="chacoTv" src="{{asset('build/img/logoChtvBlanco.svg')}}"alt="logo de Chaco Tv"></a>
                                </div>
                                <div class="col-6  justify-content-center text-center">
                                    <a href="http://ecom.chaco.gov.ar/es/" target="_blank"><img  class="ecom" src="{{asset('build/img/ecomBlanco.svg')}}" alt="logo Ecom"></a>
                                </div>
                                <div class="col-6  justify-content-center text-center">
                                    <a href="https://radioprovinciadelchaco.com.ar/" target="_blank"><img class="radio" src="{{asset('build/img/radio.svg')}}"alt="logo radio"></a>
                                </div>
                                <div class="col-6  justify-content-center text-center">
                                    <a href="https://agenciafoco.com.ar/" target="_blank"><img  class="foco" src="{{asset('build/img/foco.svg')}}" alt="logo foco"></a>
                                </div>
                        </figure>
                    </div>
                    <div class="col-sm-12  col-md-12 col-lg-5 text-center justify-content-center" >
                        <h3 id="redeSociales">Contacto</h3>
                        <div class="row">
                            <div class="col-6 text-start justify-content-center ">
                                <ul>
                                    <li>Lunes a Viernes: De 06 hs a 21 hs.</li>
                                    <li>Sabados : De 08 hs a 13 hs.</li>
                                    <li>Dirección: Roque Sáenz Peña 352/370</li>
                                </ul>
                            </div>
                            <div class="col-6 text-start justify-content-center">
                                <ul>
                                    <li>Email: at.usuarios@ecomchaco.com.ar</li>
                                    <li>Teléfono: (362) 4419300</li>
                                    <li>Fax: (362) 4458339</li>
                                    <li>Guardia: (362) 154883049</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-12  col-md-12 text-center col-lg-3 justify-content-start" >
                        <h3 id="redeSociales">Redes sociales</h3>
                        <figure class="enlaceRedes">
                            <a href="https://twitter.com/ChacoTv" target="_blank"><img class="twitter" src="{{asset('build/img/twB.svg')}}"alt="logo de twitter"></a>
                            <a href="https://www.facebook.com/chacotelevision" target="_blank"><img  class="facebook" src="{{asset('build/img/fbB.svg')}}" alt="logo de facebook"></a>
                            <a href="https://www.instagram.com/chacotv/" target="_blank"><img  class="instagram" src="{{asset('build/img/igB.svg')}}" alt="logo de instagram"></a>
                            <a href="https://www.youtube.com/user/tvpublicachaco" target="_blank"><img  class="youtube" src="{{asset('build/img/ybB.svg')}}" alt="logo de youtube"></a>
                        </figure>
                    </div>
                </div>
            </div>
        </footer>    
        {% endblock %}
    </body>
    
</html>
